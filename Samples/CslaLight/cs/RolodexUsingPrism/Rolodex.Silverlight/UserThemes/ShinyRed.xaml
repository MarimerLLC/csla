                           RadiusY="5"
                           StrokeThickness="0">
                                    <Rectangle.Fill>
                                        <LinearGradientBrush EndPoint="0,1"
                                         StartPoint="20,1"
                                         MappingMode="Absolute"
                                         SpreadMethod="Repeat">
