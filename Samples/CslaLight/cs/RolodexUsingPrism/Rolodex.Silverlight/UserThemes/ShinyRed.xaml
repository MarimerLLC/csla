                                                    <GradientStop Color="#FFFFFFFF" Offset="0.004"/>
                                                </RadialGradientBrush>
                                            </Path.Fill>
                                        </Path>
                                    </Grid>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                        <Setter Property="Background" Value="{StaticResource ShinyPieDataPointBrushGreen}"/>
                    </Style>


                </visualizationToolkit:StylePalette>

            </Setter.Value>
        </Setter>

