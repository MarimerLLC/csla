<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!--<add key="CslaDataPortalProxy" value="Csla.DataPortalClient.WcfProxy, Csla"/>-->
    <!--<add key="CslaDataPortalProxy" value="ExtendableWcfPortalForDotNet.Client.WcfProxy, ExtendableWcfPortalForDotNet"/>-->
