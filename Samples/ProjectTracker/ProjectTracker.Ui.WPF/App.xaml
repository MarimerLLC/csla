<Application x:Class="ProjectTracker.Ui.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ProjectTracker.Ui.WPF"
             StartupUri="MainWindow.xaml"
             xmlns:bxfc="http://schemas.lhotka.net/1.0.0/bxfc"
             xmlns:my="clr-namespace:WpfUI.Xaml">
  <Application.Resources>
    <bxfc:VisibilityConverter x:Key="VisibilityConverter" />
    <my:NullVisibilityConverter x:Key="NullVisibilityConverter" Invert="False" />
    <my:NullVisibilityConverter x:Key="NullVisibilityConverterI" Invert="True" />
  </Application.Resources>
</Application>
